<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
   
    <!--Texts-->
    <SolidColorBrush x:Key="textTrueBlackColor"
                     Color="#000000" />
    
    <SolidColorBrush x:Key="textBlackColor"
                     Color="#111111" />
    
    <SolidColorBrush x:Key="textDarkGrayColor"
                     Color="#585858" />

    <SolidColorBrush x:Key="textNaverExchange"
                     Color="#585858" />
    
    <SolidColorBrush x:Key="texthodler"
                     Color="#BBBBBB" />

    <SolidColorBrush x:Key="textDisable"
                     Color="#BBBBBB" />

    <SolidColorBrush x:Key="textPoint"
